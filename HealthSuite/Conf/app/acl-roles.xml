			<allow permission="dlg/staff/remove" />
			<allow permission="dlg/nurse/remove" />
			<allow permission="dlg/physician/remove" />
			<allow permission="dlg/org-main/remove" />
			<allow permission="dlg/org-ipa/remove" />
			<allow permission="dlg/org-provider/remove" />
			<allow permission="dlg/org-insurance/remove" />
