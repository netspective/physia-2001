			<allow permission="dlg/appttype/remove" />
			<allow permission="dlg/template/remove" />
			<allow permission="dlg/feescheduledataentry/update" />
			<allow permission="dlg/feescheduleentry/update" />
			<allow permission="dlg/catalog/update" />
