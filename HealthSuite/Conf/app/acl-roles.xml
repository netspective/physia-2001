			<allow permission="page/sde/stats" />
			<allow permission="page/sde/stmgrs" />
			<allow permission="page/sde/tables" />
		</permission>
		<permission id="normal-user">
			<allow permission="group/Superuser"/>
			<deny permission="group/data-admin"/>
			<deny permission="group/sde" />
		</permission>
		<permission id="Physician">
			<allow permission="group/Superuser"/>
			<deny permission="group/data-admin"/>
			<deny permission="group/sde" />
		</permission>
		<permission id="Nurse">
			<allow permission="group/Superuser"/>
			<deny permission="group/data-admin"/>
			<deny permission="group/sde" />
		</permission>
		<permission id="Staff">
			<allow permission="group/Superuser"/>
			<deny permission="group/data-admin"/>
			<deny permission="group/sde" />
		</permission>
		<permission id="Administrator">
			<allow permission="group/Superuser"/>
			<deny permission="group/sde" />
		</permission>
	</permissions>
</accesscontrol>